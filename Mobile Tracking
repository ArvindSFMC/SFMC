SELECT
   ST.MobileMessageTrackingID,
   ST.EID,
   ST.MID,
   ST.Mobile,
   ST.MessageID,
   ST.KeywordID,
   ST.CodeID,
   ST.ConversationID,
   ST.CampaignID,
   ST.Sent,
   ST.Delivered,
   ST.Undelivered,
   ST.Unsub,
   ST.OptIn,
   ST.OptOut,
   ST.Outbound,
   ST.Inbound,
   ST.CreateDateTime,
   ST.ModifiedDateTime,
   ST.ActionDateTime as 'ActionDateTime',
   ST.MessageText,
   ST.IsTest,
   ST.MobileMessageRecurrenceID,
   ST.ResponseToMobileMessageTrackingID,
   ST.IsValid,
   ST.InvalidationCode,
   ST.SendID,
   ST.SendSplitID,
   ST.SendSegmentID,
   ST.SendJobID,
   ST.SendGroupID,
   ST.SendPersonID,
   ST.SubscriberID,
   S.SubscriberKey
FROM  _smsmessagetracking ST
JOIN ent._Subscribers S ON S.SubscriberID=ST.SubscriberID
where Datediff (minute,getdate(),CreateDateTime)
where CreateDateTime > '10' 

   ST.MobileMessageTrackingID,
   ST.EID,
   ST.MID,
   ST.Mobile,
   ST.MessageID,
   ST.KeywordID,
   ST.CodeID,
   ST.ConversationID,
   ST.CampaignID,
   ST.Sent,
   ST.Delivered,
   ST.Undelivered,
   ST.Unsub,
   ST.OptIn,
   ST.OptOut,
   ST.Outbound,
   ST.Inbound,
   ST.CreateDateTime,
   ST.ModifiedDateTime,
   ST.ActionDateTime as 'ActionDateTime',
   ST.MessageText,
   ST.IsTest,
   ST.MobileMessageRecurrenceID,
   ST.ResponseToMobileMessageTrackingID,
   ST.IsValid,
   ST.InvalidationCode,
   ST.SendID,
   ST.SendSplitID,
   ST.SendSegmentID,
   ST.SendJobID,
   ST.SendGroupID,
   ST.SendPersonID,
   ST.SubscriberID,
   S.SubscriberKey
FROM  _smsmessagetracking ST
JOIN ent._Subscribers S ON S.SubscriberID=ST.SubscriberID
where Datediff (minute,getdate(),CreateDateTime)

   ST.MobileMessageTrackingID,
   ST.EID,
   ST.MID,
   ST.Mobile,
   ST.MessageID,
   ST.KeywordID,
   ST.CodeID,
   ST.ConversationID,
   ST.CampaignID,
   ST.Sent,
   ST.Delivered,
   ST.Undelivered,
   ST.Unsub,
   ST.OptIn,
   ST.OptOut,
   ST.Outbound,
   ST.Inbound,
   ST.CreateDateTime,
   ST.ModifiedDateTime,
   ST.ActionDateTime as 'ActionDateTime',
   ST.MessageText,
   ST.IsTest,
   ST.MobileMessageRecurrenceID,
   ST.ResponseToMobileMessageTrackingID,
   ST.IsValid,
   ST.InvalidationCode,
   ST.SendID,
   ST.SendSplitID,
   ST.SendSegmentID,
   ST.SendJobID,
   ST.SendGroupID,
   ST.SendPersonID,
   ST.SubscriberID,
   S.SubscriberKey
FROM  _smsmessagetracking ST
JOIN ent._Subscribers S ON S.SubscriberID=ST.SubscriberID
where Datediff (minute,getdate(),CreateDateTime)>10